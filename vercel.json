{
    "version": 2,
    "builds": [
        {
        "src": "src/index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/socket.io/(.*)",
            "dest": "https://backend-node-git-socketio-integration-the-jackson-s-five.vercel.app/socket.io/$1",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
